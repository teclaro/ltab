<div class="row mb-3">
  <div class="col-md-8 offset-md-2">
    <h1>
      University a #<%= @university_a.id %> details
    </h1>

    <div class="row mb-3">
      <div class="col">
        <a href="/university_as" class="btn btn-block btn-outline-secondary">
          Go back
        </a>
      </div>

      <div class="col">
        <a href="/university_as/<%= @university_a.id %>/edit" class="btn btn-block btn-outline-secondary">
          Edit university a
        </a>
      </div>

      <div class="col">
        <a href="/delete_university_a/<%= @university_a.id %>" class="btn btn-block btn-outline-secondary">
          Delete university a
        </a>
      </div>
    </div>

    <dl>
      <dt>
        Created at
      </dt>
      <dd>
        <%= time_ago_in_words(@university_a.created_at) %> ago
      </dd>

      <dt>
        Updated at
      </dt>
      <dd>
        <%= time_ago_in_words(@university_a.updated_at) %> ago
      </dd>
    </dl>
  </div>
</div>

