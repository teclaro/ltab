<div class="row mb-3">
  <div class="col-md-12">
    <h1>
      All team1as
    </h1>

    <a href="/team1as/new" class="btn btn-block btn-outline-secondary">
      Add a new team1a
    </a>
  </div>
</div>

<div class="row">
  <div class="col-md-12 mb-3">
    <%= render partial: "team1a_templates/team1as_search_form" %>
  </div>
</div>



<hr>

<div class="row mb-3">
  <div class="col-md-12">
    <table class="table">
      <tr>
        <th>
          ID
        </th>

        <th>
          Created at
        </th>

        <th>
          Updated at
        </th>

        <th>
        </th>
      </tr>

      <% @team1as.each do |team1a| %>
      <tr>
        <td>
          <%= team1a.id %>
        </td>

        <td>
          <%= time_ago_in_words(team1a.created_at) %> ago
        </td>

        <td>
          <%= time_ago_in_words(team1a.updated_at) %> ago
        </td>

        <td>
          <a href="/team1as/<%= team1a.id %>">
            Show details
          </a>
        </td>
      </tr>
      <% end %>
    </table>

    <div class="row mb-3">
      <div class="col-md-12">
        <%= paginate @team1as, theme: "twitter-bootstrap-4" %>
      </div>
    </div>

  </div>
</div>
