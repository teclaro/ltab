<div class="row mb-3">
  <div class="col-md-12">
    <h1>
      All team1as
    </h1>

    <a href="/team1as/new" class="btn btn-block btn-outline-secondary">
      Add a new team1a
    </a>
  </div>
</div>

<!-- Ransack Code start -->

<!-- Ransack Code end -->

<!-- Location Map Code start -->

<!-- Location Map Code end -->

<hr>

<div class="row mb-3">
  <div class="col-md-12">
    <table class="table">
      <tr>
        <th>
          ID
        </th>

        <th>
          Created at
        </th>

        <th>
          Updated at
        </th>

        <th>
        </th>
      </tr>

      <% @team1as.each do |team1a| %>
      <tr>
        <!-- Display team1a.id start -->
        <td>
          <%= team1a.id %>
        </td>
        <!-- Display team1a.id end -->

        <!-- Display team1a.created_at start -->
        <td>
          <%= time_ago_in_words(team1a.created_at) %> ago
        </td>
        <!-- Display team1a.created_at end -->

        <!-- Display team1a.updated_at start -->
        <td>
          <%= time_ago_in_words(team1a.updated_at) %> ago
        </td>
        <!-- Display team1a.updated_at end -->

        <td>
          <a href="/team1as/<%= team1a.id %>">
            Show details
          </a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
